{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-var": 2,
    "no-console": 0,
    "prefer-template": 0,
    "no-param-reassign": [2, { "props": false }],
    "max-len": [2, { "code": 300 }],
    "prefer-arrow-callback": 0,
    "consistent-return": 0,
    "global-require": 0,
    "import/no-unresolved": 0,
    "no-restricted-syntax": 0,
    "new-cap": 0,
    "arrow-body-style": 0,
    "newline-before-return": 2,
    "newline-after-var": [2, "always"],
    "space-before-blocks": [2, "always"],
    "object-property-newline": [2, { "allowMultiplePropertiesPerLine" : false}],
    "object-curly-newline": [2, "always"],
    "eol-last": [2, "always"],
    "no-multiple-empty-lines": [2, { "max": 1, "maxEOF": 1}],
    "no-unreachable": 2,
    "block-scoped-var": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-alert": 2,
    "no-eval": 2,
    "no-multi-spaces": 2,
    "no-useless-return": 2,
    "no-tabs": 0,
    "no-bitwise": 0,
    "no-plusplus": 0,
    "import/no-dynamic-require": 0,
    "no-lonely-if": 0
  }
}